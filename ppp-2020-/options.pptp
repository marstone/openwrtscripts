debug
dump
noipdefault
noauth
nodetach
nobsdcomp
nodeflate
idle 0
mppe required,no40,no56,stateless
maxfail 0

#defaultroute
refuse-eap
refuse-pap
refuse-chap
refuse-mschap
mppc
lcp-echo-failure 30
lcp-echo-interval 2
persist
usepeerdns
ip-up-script /etc/ppp/ip-up
ip-down-script /etc/ppp/ip-down
ipparam pptp-studio
mtu 1400
mru 1400
